using Gtk 4.0;

template $HistoryPage : Box {
  ScrolledWindow {
    hexpand: true;
    hscrollbar-policy: never;

    Box box {
      orientation: vertical;
      margin-top: 12;
      margin-bottom: 12;
      margin-start: 12;
      margin-end: 12;
      spacing: 12;

      ListBox list {
        styles [
          "background",
        ]
      }
    }
  }
}
